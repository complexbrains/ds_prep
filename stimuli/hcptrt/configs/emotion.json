        "value": "FacePromptPROC"
      },
      "onset": {
        "column": "face.OnsetTime"
      },
      "duration": {
        "column": "StimSlide.OnsetTime",
        "formula": [-1, "subtract", "onset"]
      },
      "nbloc": {
        "column": ["HRun*"],
        "regex": "True",
        "type": "bloc"
      }
    },
    {
      "name": "response_shape",
      "ioi": {
