{
  "TTL": "SyncSlide.OnsetTime",
  "events":[
    {
      "name": "new_bloc_shape",
      "ioi": {
        "column": "Procedure",
        "value": "ShapePromptPROC"
      },
      "onset": {
        "column": "shape.OnsetTime"
      },
      "duration": {
        "column": "StimSlide.OnsetTime",
        "formula": [-1, "subtract", "onset"]
      },
      "nbloc": {
        "column": ["HRun*"],
        "regex": "True",
        "type": "bloc"
      }
    },
    {
      "name": "new_bloc_face",
      "ioi": {
