        "column": "StimSlide.RT"
      },
      "nbloc": {
        "column": ["HRun*"],
        "regex": "True",
        "type": "bloc"
      },
      "ntrial": {
        "column": ["FaceBlock*"],
        "regex": "True"
      },
      "stim_top": {
        "column": "Top",
